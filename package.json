{
  "name": "squoll",
  "version": "0.0.0",
  "repository": "git@github.com:msfragala/squoll.git",
  "author": "Mitchell Fragala <ms.fragala@gmail.com>",
  "description": "Convert and resize images directly in the browser, via lazy-loaed web worker pools",
  "license": "MIT",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/squoll.js",
      "node": "./dist/cjs/squoll.js"
    },
    "./worker": {
      "import": "./dist/esm/worker.js",
      "node": "./dist/cjs/worker.js"
    }
  },
  "scripts": {
    "dev": "run-p dev:* demo",
    "dev:js": "node esbuild.mjs -w",
    "dev:types": "tsc --watch",
    "build": "rm -rf dist && npm run build:js && npm run build:types",
    "build:types": "tsc",
    "build:js": "node esbuild.mjs",
    "demo": "(cd demo && pnpm run dev)"
  },
  "devDependencies": {
    "esbuild": "^0.15.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  },
  "dependencies": {
    "slother": "^0.2.1"
  }
}
